<div class="inner_proper_main {% if forloop.index == 1 %} active {% endif %}join_pro_{{ forloop.index }}" data-index="{{ forloop.index }}">
  <div class="properk_join ">
    <div class="popup_inner">
      <div class="pimage_popup">
        <img src="{{ block.settings.slide_img | image_url }}">
      </div>
      <div class="pcontent_popup">
        <div class="popup_content_inner">
         <div class="popup_main-con">
          {{ block.settings.hmpopopp }}
         </div>
          {% if block.settings.bottom_icon != blank %}
          <div class="logo_main_bottom">
          <img src="{{ block.settings.bottom_icon | image_url}}">
          </div>
          {% endif %}
          <div class="main_con_bt">
            {{ block.settings.popup_content }}
          </div>
          <div class="btn_nexts">
            <button class="main_btn_next">
            {{ block.settings.btnt }}
            </button>
          </div>
      </div>
    </div>
      <div class="close_btn_m" style="cursor:pointer">
